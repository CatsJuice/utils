{
  "name": "@catsjuice/utils",
  "type": "module",
  "version": "0.1.4",
  "packageManager": "pnpm@8.6.0",
  "description": "Opinionated collection of common JavaScript / TypeScript utils by @catsjuice",
  "author": "CatsJuice <wp1298554944@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/CatsJuice/utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CatsJuice/utils.git"
  },
  "bugs": {
    "url": "https://github.com/CatsJuice/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish --registry https://registry.npmjs.org/",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.3",
    "@antfu/ni": "^0.21.3",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.16",
    "@types/throttle-debounce": "^5.0.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "lodash-es": "^4.17.21",
    "p-limit": "^4.0.0",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "throttle-debounce": "5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.31.1"
  }
}
